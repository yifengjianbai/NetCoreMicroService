{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{postId}",
      "DownstreamScheme": "http",
      "UpstreamPathTemplate": "/T/{postId}",
      "UpstreamHttpMethod": [ "Get", "Post" ],
      //"UseServiceDiscovery": true,
      "ServiceName": "TestService", //对应consul中的服务名称
      "LoadBalancerOptions": {
        "Type": "RoundRobin" //轮询
      },
      "RateLimitOptions": { //限流
        "ClientWhitelist": [],
        "EnableRateLimiting": true,
        "Period": "1s",
        "PeriodTimespan": 1,
        "Limit": 5
      },
      "FileCacheOptions": {   //使用缓存
        "TtlSeconds": 1,
        "Region": "somename"
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:5000",
    "ServiceDiscoveryProvider": { //consul访问地址
      "Host": "localhost",
      "Port": 8500,
      "Type": "Consul"
    },
    "RateLimitOptions": {
      "DisableRateLimitHeaders": false,
      "QuotaExceededMessage": "Customize Tips!",//触发限流时返回的提示
      "HttpStatusCode": 999//触发限流时返回的Code
    }
    //"ServiceDiscoveryProvider": {
    //  "Host": "localhost",
    //  "Port": 8500,
    //  "Type": "PollConsul",
    //  "PollingInterval": 100
    //}
  }
}